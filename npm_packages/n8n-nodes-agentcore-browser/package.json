{
  "name": "n8n-nodes-agentcore-browser",
  "version": "0.1.0",
  "description": "N8N community node for Amazon Bedrock AgentCore Browser automation with Playwright",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "aws",
    "amazon-bedrock",
    "agentcore",
    "browser",
    "playwright",
    "automation",
    "web-scraping"
  ],
  "license": "MIT",
  "homepage": "https://github.com/aws-samples/sample-ai-possibilities/tree/main/npm_packages/n8n-nodes-agentcore-browser",
  "author": {
    "name": "edfragas",
    "email": "edsilva@amazon.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws-samples/sample-ai-possibilities.git",
    "directory": "npm_packages/n8n-nodes-agentcore-browser"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials utils --write",
    "lint": "eslint '**/*.ts' 'package.json'",
    "lintfix": "eslint '**/*.ts' 'package.json' --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AgentcoreBrowserApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AgentcoreBrowser/AgentcoreBrowser.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.0.0",
    "@aws-sdk/client-bedrock-agentcore": "^3.930.0",
    "@aws-sdk/credential-providers": "^3.600.0",
    "@smithy/protocol-http": "^4.0.0",
    "@smithy/signature-v4": "^4.0.0",
    "@types/node": "^24.10.1",
    "playwright": "^1.48.0"
  }
}
